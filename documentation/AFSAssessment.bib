@incollection{https://doi.org/10.1016/B978-0-12-387020-9.00010-9,
title = {Chapter 10 - Estimation of Survival, Recruitment, and Population Size from Capture–Recapture Data Using the Jolly–Seber Model},
editor = {Kéry, M. and  Schaub, M.},
booktitle = {Bayesian Population Analysis using WinBUGS},
publisher = {Academic Press},
address = {Boston},
pages = {315-346},
year = {2012},
isbn = {978-0-12-387020-9},
doi = {https://doi.org/10.1016/B978-0-12-387020-9.00010-9},
url = {https://www.sciencedirect.com/science/article/pii/B9780123870209000109},
author = {Kéry, M. and  Schaub, M.},
keywords = {Abundance, capture–recapture, population size, population dynamics, recruitment, survival, entry, superpopulation},
abstract = {Publisher Summary
This chapter describes the Jolly-Seber (JS) model to estimate population size, survival and recruitment from capture-recapture data. The JS does not condition on first capture, that is, leading zeros before initial capture and the initial capture are modeled as well. There are several variants (parameterizations) of JS models that differ in how recruitment-related parameters are defined and estimated. The formulation of the JS model as a restricted occupancy model has advantages of relatively fast speed and a simple model code. The super population formulation runs relatively slowly, but it is possible to directly model the entry probability and to specify a natural priority for the size of the super population. The JS model can be formulated as a multistate model. This formulation is appealing because it allows extensions in many directions.}
}

